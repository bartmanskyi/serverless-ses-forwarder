# Default Configuration
default:

  # Simple Email Service (SES)
  ses:

    # SES Rule Set Name
    # Rule Set where the SES Rule should be added. This allow the SES Rule to
    # be added to any existing Rule Set, including the Active Rule Set.
    ruleSetName: default-rule-set
